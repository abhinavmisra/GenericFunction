package com.function.generic;

public class TestSubString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "welcome";
		int n = 3;
		for(int i=0; i<n; i++) {
			System.out.print(s.charAt(i));
		}
		System.out.println("");
		for(int i=s.length()-3; i<s.length(); i++) {
			System.out.print(s.charAt(i));
		}

	}

}
